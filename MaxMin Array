#include<iostream>
using namespace std;

void MaxMin(int arr[], int n)
{
    int Max=arr[0];
    int Min=arr[0];
    
    for(int i=0;i<n;i++)
    {
        if(Max<arr[i])
         Max=arr[i];
    }
    cout<<Max;
}
int main()
{
    int arr[5]={1,2,3,40,4};
    int n = sizeof(arr)/sizeof(arr[0]);
    MaxMin(arr,n);
}
